{
  "name": "@samatech/vue3-eth",
  "version": "0.6.0",
  "description": "Vue3 library for building Dapps in an ES module environment",
  "main": "dist/vue3-eth.umd",
  "module": "dist/vue3-eth.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "build": "rimraf dist && tsc && rollup -c rollup.config.js",
    "dev": "concurrently --kill-others \"tsc --watch\" \"npm run example:dev\"",
    "example:dev": "npm -C example run dev",
    "test": "jest",
    "lint": "eslint --ext .js,.ts .",
    "format": "prettier -w \"{**/*,*}.{ts,js,json}\"",
    "format:check": "prettier --check \"{**/*,*}.{ts,js,json}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samatechtw/vue3-eth.git"
  },
  "keywords": [
    "fetch",
    "api"
  ],
  "author": "Sam Pullman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samatechtw/vue3-eth/issues"
  },
  "homepage": "https://github.com/samatechtw/vue3-eth#readme",
  "peerDependencies": {
    "vue": "^3.2.31"
  },
  "dependencies": {
    "@metamask/detect-provider": "^1.2.0",
    "big.js": "^6.1.1",
    "ethers": "5.6.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/big.js": "^6.1.3",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.5.5"
  }
}
