{
  "name": "@samatech/vue3-eth",
  "version": "0.4.2",
  "description": "Vue3 library for building Dapps in an ES module environment",
  "main": "dist/vue3-eth.umd",
  "module": "dist/vue3-eth.mjs",
  "types": "dist/vue3-eth.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c rollup.config.js",
    "example:dev": "npm -C example run dev",
    "example:build": "npm -C example run build",
    "build:all": "npm run build && npm run plugin:build",
    "build:test": "npm run build && jest",
    "test": "jest",
    "release": "npx git-ensure -a && npx bumpp --commit --tag --push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samatechtw/vue3-eth.git"
  },
  "keywords": [
    "fetch",
    "api"
  ],
  "author": "Sam Pullman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samatechtw/vue3-eth/issues"
  },
  "homepage": "https://github.com/samatechtw/vue3-eth#readme",
  "peerDependencies": {
    "vue": "^3.2.6"
  },
  "dependencies": {
    "@metamask/detect-provider": "^1.2.0",
    "ethers": "5.4.5",
    "big.js": "^6.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "jest": "^27.0.6",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2"
  }
}
